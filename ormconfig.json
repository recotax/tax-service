{
  "type": "mysql2",
  "host": "${DB_HOST}",
  "port": "${DB_PORT}",
  "username": "${DB_USER}",
  "password": "${DB_PASSWORD}",
  "database": "${DB_NAME}",
  "synchronize": true,
  "logging": false,
  "entities": ["src/models/*.ts"],
  "migrations": ["src/migrations"],
  "cli": {
    "entitiesDir": "src/models"
  }
}
